{
  "name": "kolibri-theme",
  "version": "1.3.4",
  "description": "",
  "scripts": {
    "coverage": "cross-env NODE_ENV=test nyc mocha",
    "cucumber": "cross-env NIGHTWATCH_ENV=chrome cucumber-js tests/cucumber/features/**/*.feature --require cucumber.conf.js --require tests/cucumber/step-definitions --format @cucumber/pretty-formatter --format json:.reports/cucumber/report.json",
    "e2e": "nightwatch",
    "format": "prettier --check \"{src,tests}/**\" --ignore-unknown",
    "lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
    "reinstall": "npm run remove && npm install",
    "reinstall:kolibri": "rm -rf node_modules/@kolibri && npm update @kolibri/lib @kolibri/solid",
    "remove": "git clean -f -d -X",
    "update": "ncu -t minor -u && npm install && ncu",
    "build": "cross-env NODE_ENV=development webpack",
    "start": "npm run serve -- --open",
    "serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map --no-hot",
    "preview": "vite preview",
    "test": "cross-env NODE_ENV=test mocha",
    "prepare:husky": "husky install",
    "commit-msg": "commitlint",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@kolibri/lib": "1.0.11-rc.1",
    "@kolibri/solid": "1.0.11-rc.1",
    "@leanup/cli": "1.3.6",
    "@leanup/cli-solid": "1.3.6",
    "@leanup/form": "1.3.6",
    "@leanup/lib": "1.3.6",
    "@leanup/openapi-demo": "1.3.5",
    "monaco-editor": "0.32.1",
    "monaco-editor-samples": "0.0.1",
    "monaco-editor-webpack-plugin": "7.0.1",
    "navigo": "8.11.1",
    "npm-check-updates": "12.5.2",
    "prettier": "2.5.1",
    "rollup": "2.70.0",
    "rollup-plugin-multi-input": "1.3.1",
    "rollup-plugin-sass": "1.2.10",
    "rxjs": "7.5.4",
    "solid-js": "1.3.10"
  },
  "devDependencies": {
    "@leanup/cli": "1.3.6",
    "@leanup/cli-solid": "1.3.6",
    "@leanup/git-hooks": "1.3.6",
    "@leanup/stack": "1.3.6",
    "@leanup/stack-solid": "1.3.6",
    "@leanup/stack-webpack": "1.3.6",
    "@types/clean-css": "4.2.5",
    "@types/prettier": "^2.4.4",
    "@unocss/preset-mini": "0.27.2",
    "@unocss/transformer-directives": "0.27.2",
    "@unocss/webpack": "0.27.2",
    "clean-css": "5.2.4",
    "npm-check-updates": "12.5.2",
    "sass": "1.49.9",
    "selenium-server": "3.141.59",
    "typescript": "4.6.2"
  }
}
